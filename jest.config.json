{
  "snapshotSerializers": ["enzyme-to-json/serializer"],
  "setupFiles": [
      "raf/polyfill",
      "<rootDir>/src/tests/setupTests.ts"
  ],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ]
}